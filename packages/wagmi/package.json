{
    "name": "@dae/wagmi",
    "version": "0.0.1",
    "description": "Wagmi hooks for the DAE (Decentralized Autonomous Education) Project.",
    "scripts": {
        "build": "true",
        "check": "tsc --pretty --noEmit",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "dependencies": {
        "viem": "1.0.6"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./*": [
            "./src/*.ts",
            "./src/*.tsx",
            "./src/*/index.ts",
            "./src/*/index.tsx"
        ]
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/*",
                "src/*/index"
            ]
        }
    },
    "devDependencies": {
        "@dae/typescript-config": "workspace:*",
        "@types/node": "18.11.18",
        "@types/react": "18.0.22",
        "@types/react-dom": "18.0.7",
        "@dae/abi": "workspace:*",
        "@wagmi/core": "1.2.0",
        "typescript": "5.0.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "next": "13.4.1",
        "wagmi": "1.2.0"
    },
    "files": [
        "src"
    ],
    "peerDependencies": {
        "@wagmi/core": "1.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "wagmi": "1.2.0"
    },
    "peerDependenciesMeta": {
        "@wagmi/core": {
            "optional": false
        },
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        },
        "wagmi": {
            "optional": true
        }
    },
    "author": "",
    "license": "MIT"
}